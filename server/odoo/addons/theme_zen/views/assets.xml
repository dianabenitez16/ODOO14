<?xml version="1.0" encoding="utf-8"?>

<odoo>

     <template id="assets_frontend" name="My style" inherit_id="website.assets_frontend">
    <xpath expr="//link[last()]" position="after">

        <link rel="stylesheet" type="text/scss" href="/theme_zen/static/src/css/animate.min.css"/>
        <link rel="stylesheet" type="text/scss" href="/theme_zen/static/src/css/foundation.min.css"/>
        <link rel="stylesheet" type="text/scss" href="/theme_zen/static/src/css/owl.carousel.min.css"/>
        <link rel="stylesheet" type="text/scss" href="/theme_zen/static/src/css/owl.theme.default.min.css"/>
        <link rel="stylesheet" type="text/scss" href="/theme_zen/static/src/css/style.css"/>

        </xpath>
         <xpath expr="//script[last()]" position="after">
              <script type="text/javascript" src="/theme_zen/static/src/js/foundation.min.js"/>
             <script type="text/javascript" src="/theme_zen/static/src/js/owl.carousel.js"/>
<!--             <script type="text/javascript" src="/theme_zen/static/src/js/owl.carousal.min.js"/>-->
             <script type="text/javascript" src="/theme_zen/static/src/js/custom.js"/>
             <script type="text/javascript" src="/theme_zen/static/src/js/custom_slider.js"/>
             <script type="text/javascript" src="/theme_zen/static/src/js/custom_nav.js"/>

         </xpath>
    </template>

</odoo>